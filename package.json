{
  "name": "cybersource-soap-node-sdk",
  "version": "2.0.0",
  "description": "Unofficial Node.js SDK for CyberSource SOAP API with P12 certificate support",
  "main": "index.js",
  "types": "./CybersourceApi.d.ts",
  "scripts": {
    "test": "jest",
    "test:all": "jest",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:watch": "jest --watch --testPathIgnorePatterns=test/integration",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=test/integration",
    "test:credentials": "node test/interactive/credential-test.js",
    "test:certificates": "node test/interactive/certificate-test-simple.js",
    "test:certificates-interactive": "node test/interactive/certificate-test-interactive.js",
    "convert:certificates": "node scripts/convert-certificates.js",
    "examples:certificates": "node examples/certificate-examples.js",
    "examples:subscription-info": "node examples/subscription-info-example.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test && npm run lint",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "soap": "^1.4.1",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "inquirer": "^8.2.6",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "prettier": "^3.2.5"
  },
  "keywords": [
    "cybersource",
    "payment",
    "gateway",
    "credit-card",
    "soap",
    "api",
    "p12",
    "certificate",
    "nodejs",
    "sdk",
    "payment-processing",
    "ecommerce"
  ],
  "author": "Jose Abraham GarcÃ­a",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/joseabraham/cybersource-soap-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/joseabraham/cybersource-soap-node-sdk/issues"
  },
  "homepage": "https://github.com/joseabraham/cybersource-soap-node-sdk#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "index.js",
    "CybersourceApi.js",
    "CybersourceApi.d.ts",
    "common.js",
    "model/",
    "api/",
    "json/"
  ]
}
